	<variable name="CurrentBalance" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{REPORT_COUNT} == 1
    ? (
        $V{PreviousBalance} != null 
            ? new java.math.BigDecimal($V{PreviousBalance}.toString()) // Use PreviousBalance
                .subtract(
                    $F{PART_TRAN_TYPE} != null && $F{PART_TRAN_TYPE}.equals("debit") 
                        ? new java.math.BigDecimal($F{TRAN_AMT} == null ? "0" : $F{TRAN_AMT}.toString()) // Deduct debit
                        : java.math.BigDecimal.ZERO // No deduction if not debit
                )
                .add(
                    $F{PART_TRAN_TYPE} != null && $F{PART_TRAN_TYPE}.equals("credit") 
                        ? new java.math.BigDecimal($F{TRAN_AMT} == null ? "0" : $F{TRAN_AMT}.toString()) // Add credit
                        : java.math.BigDecimal.ZERO // No addition if not credit
                )
            : java.math.BigDecimal.ZERO // Default to 0 if PreviousBalance is null
      )
    : (
        $V{CurrentBalance} != null 
            ? $V{CurrentBalance} // Use CurrentBalance
                .subtract(
                    $F{PART_TRAN_TYPE} != null && $F{PART_TRAN_TYPE}.equals("debit") 
                        ? new java.math.BigDecimal($F{TRAN_AMT} == null ? "0" : $F{TRAN_AMT}.toString()) // Deduct debit
                        : java.math.BigDecimal.ZERO // No deduction if not debit
                )
                .add(
                    $F{PART_TRAN_TYPE} != null && $F{PART_TRAN_TYPE}.equals("credit") 
                        ? new java.math.BigDecimal($F{TRAN_AMT} == null ? "0" : $F{TRAN_AMT}.toString()) // Add credit
                        : java.math.BigDecimal.ZERO // No addition if not credit
                )
            : java.math.BigDecimal.ZERO // Default to 0 if CurrentBalance is null
      )]]></variableExpression>
	</variable>
	<variable name="Debit " class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{PART_TRAN_TYPE} != null && $F{PART_TRAN_TYPE}.equals("debit") 
    ? new java.math.BigDecimal($F{TRAN_AMT} == null ? "0" : $F{TRAN_AMT}.toString()) 
    : java.math.BigDecimal.ZERO]]></variableExpression>
	</variable>
	<variable name="Credit" class="java.math.BigDecimal">
