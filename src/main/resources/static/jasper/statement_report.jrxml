	<queryString>
		<![CDATA[WITH PreviousBalance AS (
    SELECT 
        TO_CHAR(TRAN_DATE, 'DD-MM-YYYY') AS PreviousDate, 
        TRAN_DATE_BAL AS PreviousBalance
    FROM (
        SELECT 
            TRAN_DATE, 
            TRAN_DATE_BAL
        FROM 
            DAB_VIEW
        WHERE 
            ACCT_NUM = $P{ACCT_NO}
            AND TO_DATE($P{FROM_DATE}, 'DD-MM-YYYY') - 1 BETWEEN TRAN_DATE AND END_TRAN_DATE
        UNION ALL
        SELECT 
            TRAN_DATE, 
            TRAN_DATE_BAL
        FROM 
            DAB_VIEW
        WHERE 
            ACCT_NUM = $P{ACCT_NO}
            AND TRAN_DATE_BAL IS NOT NULL
            AND TRAN_DATE < TO_DATE($P{FROM_DATE}, 'DD-MM-YYYY')
    )
    ORDER BY 
        TRAN_DATE DESC
    FETCH FIRST 1 ROW ONLY
),
TemplateData AS (
    SELECT  ORGANIZATION_NAME,ADDRESS_LINE_1,
    ADDRESS_LINE_2,CITY,STATE,COUNTRY,CUSTOMER_ID,
    FOOTER_CONTENT_1,FOOTER_CONTENT_2,FOOTER_CONTENT_3
    FROM TEMPLATE_TABLE
    WHERE TEMPLATE_ID = $P{TEMPLATE_ID}
)
SELECT 
    TWV.*,
    PB.PreviousBalance,
    PB.PreviousDate,
    TD.*
FROM 
    TRMWORK_VIEW TWV
LEFT JOIN 
    PreviousBalance PB
ON 
    TWV.ACCT_NUM = $P{ACCT_NO}
LEFT JOIN 
    TemplateData TD
ON 
    1 = 1 -- Adjust this join condition if required
WHERE 
    TWV.ACCT_NUM = $P{ACCT_NO}
    AND TWV.TRAN_DATE BETWEEN TO_DATE($P{FROM_DATE}, 'DD-MM-YYYY') AND TO_DATE($P{TO_DATE}, 'DD-MM-YYYY')
ORDER BY 
    TWV.TRAN_DATE]]>
	</queryString>
